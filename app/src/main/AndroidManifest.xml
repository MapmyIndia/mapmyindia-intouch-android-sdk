<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mapmyindia.intouchsdkdemo">

    <application
        android:name=".IntouchSampleApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="com.mapmyindia.intouchsdkdemo.MainActivity"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <provider
            android:name="com.mapmyindia.sdk.intouch.InTouchProvider"
            android:authorities="${applicationId}.InTouchProvider"
            android:enabled="true"
            android:exported="false" />
    </application>

</manifest>